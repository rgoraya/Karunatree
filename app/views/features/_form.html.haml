- form_for @feature do |f|
  = f.error_messages
  %p
    = f.label :locator
    %br
    = f.text_field :locator
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.submit "Submit"
