- title "Characters"

%table
  %tr
    %th User
    %th Saved kml
    %th Scene name
    %th Subscene
    %th Feature
  - for character in @characters
    %tr
      %td= h character.user_id
      %td= h character.saved_kml
      %td= h character.scene_name
      %td= h character.subscene
      %td= h character.feature_id
      %td= link_to 'Show', character
      %td= link_to 'Edit', edit_character_path(character)
      %td= link_to 'Destroy', character, :confirm => 'Are you sure?', :method => :delete

%p= link_to "New Character", new_character_path
