- form_for @character do |f|
  = f.error_messages
  %p
    = f.label :user_id
    %br
    = f.text_field :user_id
  %p
    = f.label :saved_kml
    %br
    = f.text_area :saved_kml
  %p
    = f.label :scene_name
    %br
    = f.text_field :scene_name
  %p
    = f.label :subscene
    %br
    = f.text_field :subscene
  %p
    = f.label :feature_id
    %br
    = f.text_field :feature_id
  %p
    = f.submit "Submit"
